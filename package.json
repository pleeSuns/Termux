{
  "name": "shapeshifter",
  "version": "1.0.5",
  "license": "Apache-2.0",
  "repository": "https://github.com/alexjlockwood/ShapeShifter",
  "author": "Alex Lockwood <alexjlockwood@gmail.com>",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "node --max-old-space-size=8192 ./node_modules/.bin/ng serve",
    "build": "node --max-old-space-size=8192 ./node_modules/.bin/ng build -prod",
    "test": "node --max-old-space-size=8192 ./node_modules/.bin/ng test --browser ChromeHeadless",
    "test-once":
      "node --max-old-space-size=8192 ./node_modules/.bin/ng test --browser ChromeHeadless --single-run --code-coverage",
    "lint": "node --max-old-space-size=8192 ./node_modules/.bin/ng lint --type-check",
    "e2e": "node --max-old-space-size=8192 ./node_modules/.bin/ng e2e",
    "deploy":
      "node --max-old-space-size=8192 ./node_modules/.bin/ng build -prod && cp ./CNAME ./dist/CNAME && ngh",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "bundle-report":
      "node --max-old-space-size=8192 ./node_modules/.bin/ng build --prod --target=production --environment=prod --aot --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
  "dependencies": {
    "@angular/animations": "^4.4.5",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^4.4.5",
    "@angular/compiler": "^4.4.5",
