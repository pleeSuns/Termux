    "@types/node": "10.1.1",
    "@types/polylabel": "^1.0.5",
    "@types/tinycolor2": "^1.4.0",
    "angular-cli-ghpages": "^0.5.2",
    "codelyzer": "^4.3.0",
    "coveralls": "^3.0.1",
    "deep-freeze-strict": "^1.1.1",
