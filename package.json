{
  "name": "shape-shifter",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "test-once": "./node_modules/@angular/cli/bin/ng test --single-run --code-coverage",
    "build": "ng build -prod --base-href /ShapeShifter/",
    "deploy": "ng build -prod --base-href /ShapeShifter/ && ngh",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "private": true,
  "dependencies": {
